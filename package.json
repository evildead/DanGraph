{
  "name": "dangraph",
  "version": "1.0.2",
  "engines": {
    "node": ">=16"
  },
  "description": "Utilities for directed and undirected graphs",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register --timeout 10000 --colors test/**/*.test.ts",
    "rimraf": "./node_modules/rimraf/bin.js",
    "clean": "rimraf build/",
    "build": "npm run clean && tsc && npm i --package-lock-only",
    "prepublishOnly": "npm test",
    "build:doc": "jsdoc2md --files ./src/*/*.ts --configure ./jsdoc2md.json > ./docs/code-documentation.md",
    "lint": "eslint ./src --ext ts",
    "lint-fix": "eslint ./src --fix --cache"
  },
  "keywords": [
    "graph-theory",
    "nodes",
    "arcs",
    "node.js",
    "typescript"
  ],
  "author": "Danilo Carrabino",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evildead/DanGraph"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/eslint-plugin-prettier": "^3.1.3",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^8.2.2",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^2.1.0",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^5.0.3",
    "mocha": "^8.3.2",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
