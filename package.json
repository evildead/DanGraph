{
  "name": "dangraph",
  "version": "1.0.0",
  "description": "Utilities for directed and undirected graphs",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register --timeout 10000 --colors test/**/*.test.ts",
    "build": "rm -rf lib && tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "build:doc": "jsdoc2md --files ./src/*/*.ts --configure ./jsdoc2md.json > ./docs/code-documentation.md",
    "lint": "eslint . --ext ts"
  },
  "keywords": [
    "graph-theory",
    "nodes",
    "arcs",
    "node.js",
    "typescript"
  ],
  "author": "Danilo Carrabino",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evildead/DanGraph"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^5.0.3",
    "mocha": "^6.2.3",
    "prettier": "^1.19.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.150",
    "lodash": "^4.17.15"
  }
}
